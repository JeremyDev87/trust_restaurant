{
  "name": "clean-plate-mcp",
  "version": "1.0.0",
  "description": "식약처 공인 데이터 기반 식당 위생 정보 조회 MCP 서버",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "clean-plate-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:vercel": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:server": "tsx watch --env-file=.env src/main.ts",
    "start": "node --env-file=.env dist/index.js",
    "start:server": "node --env-file=.env dist/src/main.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "food-safety",
    "hygiene",
    "restaurant",
    "korea",
    "claude",
    "ai"
  ],
  "author": "Clean Plate MCP Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/clean-plate-mcp.git"
  },
  "homepage": "https://github.com/username/clean-plate-mcp#readme",
  "bugs": {
    "url": "https://github.com/username/clean-plate-mcp/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@nestjs/common": "^11.1.10",
    "@nestjs/core": "^11.1.10",
    "@nestjs/platform-express": "^11.1.10",
    "@vercel/kv": "^3.0.0",
    "express": "^5.2.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@nestjs/testing": "^11.1.10",
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/node": "^5.5.16",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.0",
    "globals": "^16.0.0",
    "msw": "^2.6.0",
    "prettier": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
